      <name>Ctrl-A</name>
      <desc>ALT</desc>
      <binding>
        <command>nasal</command>
        <script>l410autopilot.alt_btn()</script>
      </binding>
    </key>
    
    <key n="17">
      <name>Ctrl-Q</name>
      <desc>FD</desc>
      <binding>
